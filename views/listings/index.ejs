<%- include('../partials/head') %>

    <div class="container">
        <h1>Here's all the listings</h1>
        <div class="houses-con">
            <% listings.forEach((listing) => { %>

                <div class="card mt-3" id="houses" style="width: 18rem;">
                    <img src="<%= listing.imgUrl%>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title">$<%= listing.price %></h5>
                      <p class="card-text"><%= listing.streetAddress %> <%= listing.city  %></p>
                      <a href="/listings/<%= listing._id%>" class="btn btn-primary">View Details</a>
                    </div>
                  </div>

            <% }) %>  
        </div>
    </div>

<%- include('../partials/footer') %>